<div class="container">
  <div class="row">
    <h4 class="header col-lg-2 text-center bg-light my-4 ml-5">会員一覧</h4>
  </div>

　<table class="col-lg-10 mx-auto">
  <thead>
    <tr>
      <th>会員id</th>
      <th>氏名</th>
      <th>税抜価格</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= link_to customer.name, admin_customer_path(customer)%></td>
      <td><%= customer.email %></td>
      <td><%= customer.is_active ? '有効' : '退会' %></td>
    </tr>
    <% end %>
  </tbody>
 </table>